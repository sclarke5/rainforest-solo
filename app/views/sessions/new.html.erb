<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>

<%= form_for :session, url: sessions_path do |f| %>

  <div class="field">
    <p><%= f.label :email %> </p>
    <p><%= f.email_field :email %></p>
  </div>

  <div class="field">
    <p><%= f.label :password %> </p>
    <p><%= f.password_field :password %></p>
  </div>

  <div class="field">
    <p> <%= f.submit 'Login'%></p>
  </div>
<% end %>
